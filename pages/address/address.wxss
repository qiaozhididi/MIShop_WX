page{
  background-color: #f5f5f5;
  font-size: 16px;
}
.address{
  width: 90%;
  margin:20px auto;
  background-color: #fff;
  border-radius: 10px;
  position: relative;
}
.address .getSiteIndex{
  width: 85%;
  height: 81px;
  position: absolute;
  left: 0;
  right: 0;
}
.address .item{
  padding: 20px 10px;
  overflow: hidden;
}
.address .item .info text{
  margin-right: 5px;
}
.address .item .site text{
  margin-right: 5px;
  font-size: 14px;
  color: #5B5B5C;
}
.address .item image{
  width: 30px;
  float: right;
  margin-top: -35px;
}
.down{
  width: 100%;
  height: 60px;
  background-color: #fff;
  /* border-top: 1px solid #000; */
  position: fixed;
  bottom: 0;
  left: 0;
}
.down .btn{
  margin: 5px auto 0;
  color: #fff;
  text-align: center;
  line-height: 50px;
  width: 95%;
  background: linear-gradient(to right,#FF7D01,#FC592E);
  border-radius: 50px;
}